$("#<%=@event_type.depth_name(@number+1)%>_level").html("<%= escape_javascript(options_for_select(@x_level_selection.collect { |d| [d.short_description, d.id] })) %>");
<% for i in (@number + 2)..5 %>
<% @x_level_selection = Array.new %>
$("#<%=@event_type.depth_name(i)%>_level").html("<%= escape_javascript(options_for_select(@x_level_selection.collect { |d| [d.short_description, d.id] })) %>");
<% end %>